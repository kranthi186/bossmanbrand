          a {
            border-bottom: 0;
          }
        }
      }
      ul.level-1 {
        text-align: center;
        > li {
          &.hasChild {
            > a {
              &:after {
                content: '';
                display: inline-block;
                width: 8px;
                height: 8px;
                border: 2px solid #777;
                border-left: 0;
                border-bottom: 0;
                transform: rotate(45deg);
                position: relative;
                top: -2px;
                left: 5px;
              }
            }
          }
          > a {
            
          }
          ul.level-2 {
            position: absolute;
            left: -100%;
            top: 0;
            transition: 0.35s;
            z-index: 2000;
            background: white;
            width: 100%;
            height: 100%;
            max-height: 100%;
            overflow: auto;
            &.to-show {
              left: 0;
            }
            > li {
              &.title-link {
                display: block;
                margin-bottom: 6px;
                > a {
                  font-weight: bold;
                  border-bottom: 0;
                }
              }
              &.menu_content_head {
                display: flex;
                margin-left: -10px;
                margin-right: -10px;
                width: calc(100% + 10px);
                a {
                  border-bottom: 0;
                  padding: 10px;
                }
                .back {
                  padding: 2px 10px;
                  &:before {
                    content: "";
                    display: inline-block;
                    width: 10px;
                    height: 10px;
                    border: 2px solid black;
                    border-right: 0;
                    border-bottom: 0;
                    transform: rotate(-45deg);
                    position: relative;
                    top: -2px;
                    right: 2px;
                  }
                }
              }
              > a {
                
              }
              &.hasChild {
                > a {
                  &:after {
                    content: '';
                    display: inline-block;
                    width: 8px;
                    height: 8px;
                    border: 1px solid #777;
                    border-left: 0;
                    border-bottom: 0;
                    transform: rotate(135deg);
                    position: relative;
                    top: -4px;
                    left: 10px;
                    transition: 0.35s;
                  }
                  &.to-slide {
                    &:after {
                      transform: rotate(-45deg);
                      top: -1px;
                    }
                  }
                }
              }
              ul.level-3 {
                display: none;
                > li {
                  > a {
                    font-size: 92%;
