    display:grid;
    grid-template-columns:1fr 1fr 1fr 1fr;
    grid-column-gap:20px;
    grid-row-gap:20px;
    justify-items:center;
}
.product_more_content .videos_item{
    width:227px;
    height:405px;
    position:relative;
    z-index:998
}
.product_more_content .videos_item:after,.product_more_content .videos_item:before{
    content:'';
    display:block;
    width:100%;
    height:100%;
    position:absolute
}
.product_more_content .videos_item:before{
    z-index:999;
    background:rgba(0,0,0,.7)
}
.product_more_content .videos_item:after{
    background:url(play_video.png) center center no-repeat;
    -webkit-background-size:67px 64px;
    background-size:67px 64px;
    z-index:1000;
    top:0;
    left:0;
    cursor:pointer
}
.product_more_content .videos_item.active:after,.product_more_content .videos_item.active:before{
    display:none
}
