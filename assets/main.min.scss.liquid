    clear:both;
    visibility:hidden;
    line-height:0;
    height:0
}
.btn:before,.header.media .icons a span.menu:before,.menu.media .menu_content_links a.arrow:after,.owl-nav span:before,.pagination ul li.active:after,.pagination ul li.pages:before,label:after{
