.timerarea {
    float: left;
    width: 100%;
    background: #FDF3D0;
    color: #000000;
    border-radius: 4px;
    padding-top: 7px;
    padding-bottom: 7px;
    padding-left: 6px;
    border: 1px solid #FDD763;
    font-size: 12px;
    margin-bottom: 2%;
    box-sizing: border-box;
}
.imagetimer {
    float: left;
    width: 5%;
    text-align: center;
}
img.imageinnertimer {
    width: 100%;
    max-width: 22px;
}
.timertext {
    float: left;
    width: 95%;
    padding: 1%;
    box-sizing: border-box;
}
@media (max-width: 999px){
.timerarea {
    font-size: 11px !important;
    line-height: 14px;
}
.timertext {
    padding-left: 3px;
    box-sizing: border-box;
}
}

/* WHY FROM US */
.order-summary__sections {
    height: auto;
}
.why-us-title {
    float: left;
    width: 100%;
    text-align: center;
    font-size: 19px;
    font-weight: bold;
    margin-bottom: 3%;
}
.why-us-title {
    position: relative;
}
.why-us-title:before {
    border-top: 1px solid #d7d4d4;
    content: '';
    margin: 0px auto;
    top: 50%;
    left: 0px;
    z-index: 1;
    position: absolute;
    width: 25%;
}
.why-us-title:after {
    border-top: 1px solid #d7d4d4;
    content: '';
    margin: 0px auto;
    top: 50%;
    right: 0px;
    z-index: 1;
    position: absolute;
    width: 25%;
}
.why-us-title span {
    position: relative;
    z-index: 33;
    padding-left: 1%;
    padding-right: 1%;
}
.why-us-row {
    float: left;
    width: 100%;
    margin-bottom: 2%;
    padding-top: 4%;
    border-radius: 1px;
}
.why-us-img {
    float: left;
    text-align: center;
    width: 20%;
}
.why-us-img img {
    width: 100%;
    max-width: 100px;
}
.why-us-desc {
    float: right;
    width: 80%;
    padding: 2%;
    box-sizing: border-box;
}
.why-us-subtitle {
    width: 100%;
    float: left;
    font-weight: bold;
    margin-bottom: 2%;
}



@media all and (max-width: 999px){
.why-us{
	display:none;  
}
.why-us-title {
    float: left;
    width: 100%;
    text-align: center;
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 3%;
    margin-top: 3%;
    position: relative;
}
.why-us-row {
    float: left;
    width: 100%;
    margin-bottom: 2%;
    padding-top: 4%;
    padding-bottom: 4%;
    border-radius: 1px;
    padding: 3%;
    box-sizing: border-box;
}
}