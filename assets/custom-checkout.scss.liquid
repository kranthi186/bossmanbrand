.bc {
  margin: 0px;
  padding: 0px;
  list-style-type: none;
  width: 100%;
  max-width: 800px;
  height: 120px;
  counter-reset: breadcrumb;
  position: relative;
}
.bc .bc_item {
  float: left;
  border: 3px solid black;
  border-right: 0px;
  padding: 20px;
  width: calc(25% - 44px);
  text-align: center;
  position: relative;
}
.bc .bc_item:last-child {
  border-right: 3px solid black;
}
.bc .bc_complete {
  background: white;
  color: black;
}

